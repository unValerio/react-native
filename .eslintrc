{
  "env": {
    "browser": true,
    "es6": true,
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
    },
    "ecmaVersion": 2018,
    "sourceType": "module",
  },
  "plugins": [
    "react",
    "jest",
  ],
  "rules": {
    "react/prop-types": 0,
  }
}
